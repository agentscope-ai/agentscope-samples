# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# LLM API Keys
DASHSCOPE_API_KEY=your_dashscope_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Embedding API Keys
SILICONFLOW_API_KEY=your_siliconflow_api_key_here

# File Loader API Keys
# Unstructured API (optional - if not set, will use local processing mode)
# Get your API key from: https://unstructured.io/
UNSTRUCTURED_API_KEY=your_unstructured_api_key_here
UNSTRUCTURED_API_URL=https://api.unstructured.io/general/v0/general

# Web Crawler API Keys
# FireCrawl API Key (required for FireCrawlCrawler)
# Get your API key from: https://www.firecrawl.dev/
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Jina AI Reader API Key (required for JinaCrawler)
# Get your API key from: https://jina.ai/reader/
JINA_API_TOKEN=your_jina_api_token_here
# Alternative: JINAAI_API_KEY=your_jina_api_key_here

# Directory Configuration
TMP_FILE_STORAGE_DIR=./tmp
UPLOAD_DIR=./uploads

# Milvus Configuration (optional, can be set in config.yaml)
# MILVUS_URI=http://localhost:19530
# MILVUS_TOKEN=root:Milvus
